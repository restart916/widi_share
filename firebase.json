{
  "hosting": {
    "public": "firebase-hosting",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**/*.@(jpg|jpeg|gif|png|svg)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=86400"
      } ]
    }]
  }
}
